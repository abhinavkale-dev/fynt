    "@types/node": "^22.15.3",
    "@types/pg": "^8.16.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "baseline-browser-mapping": "^2.10.0",
    "eslint": "^9.39.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.2"
