{
  "name": "@fynt/web",
  "version": "0.1.0",
  "description": "Fynt Web - Visual workflow automation editor",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "BROWSERSLIST_IGNORE_OLD_DATA=true BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true next dev --port 3000",
    "build": "rm -rf .next-build && BROWSERSLIST_IGNORE_OLD_DATA=true BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true NEXT_DIST_DIR=.next-build next build --webpack",
    "start": "BROWSERSLIST_IGNORE_OLD_DATA=true BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true NEXT_DIST_DIR=.next-build next start",
    "lint": "eslint --max-warnings 0",
    "check-types": "BROWSERSLIST_IGNORE_OLD_DATA=true BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true next typegen && tsc --noEmit"
  },
  "dependencies": {
    "@number-flow/react": "^0.5.12",
