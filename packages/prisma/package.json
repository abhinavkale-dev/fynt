{
  "name": "@repo/prisma",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "echo 'Skipping Prisma build - using generated client'",
    "generate": "prisma generate",
    "push": "DOTENV_CONFIG_PATH=../../.env prisma --config ../../prisma.config.ts db push",
    "seed": "tsx src/seed.ts",
    "hard-delete-airtable-email": "tsx src/hard-delete-airtable-email.ts"
  },
  "dependencies": {
    "@prisma/client": "^7.0.0",
    "@prisma/adapter-pg": "^7.0.0",
    "pg": "^8.13.1",
    "dotenv": "^17.2.2"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "prisma": "^7.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.0.0"
  }
}
