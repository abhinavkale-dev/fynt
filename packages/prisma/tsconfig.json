{
  "extends": "@repo/typescript-config/base.json",
  "compilerOptions": {
    "outDir": ".",
    "rootDir": ".",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "declaration": false,
    "declarationMap": false,
    "sourceMap": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["src/index.ts", "src/seed.ts"],
  "exclude": ["node_modules", "generated", "migrations"]
}
